{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "businessSaleNew Schema",
  "required": [
    "quantity_sold",
    "sale_price",
    "business_percentage",
    "sale_date"
  ],
  "properties": {
    "id": {
      "type": "integer"
    },
    "business_id": {
      "type": "integer"
    },
    "product_id": {
      "type": "integer"
    },
    "quantity_sold": {
      "type": "integer",
      "minimum": 1
    },
    "sale_price": {
      "type": "number",
      "minimum": 0
    },
    "business_percentage": {
      "type": "number",
      "maximum": 100,
      "minimum": 0
    },
    "sale_date": {
      "type": "string",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "examples": [
    {
      "id": 1,
      "business_id": 789,
      "product_id": 123,
      "quantity_sold": 10,
      "sale_price": 49.99,
      "business_percentage": 10.5,
      "sale_date": "2024-05-30T10:00:00Z",
      "updated_at": "2024-05-30T10:00:00Z",
      "created_at": "2024-05-30T10:00:00Z"
    }
  ]
}
