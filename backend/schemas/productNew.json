{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "productNew Schema",
  "required": ["name"],
  "properties": {
    "userId": {
      "type": "integer"
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "description": {
      "type": "string"
    },
    "price": {
      "type": "number"
    },
    "cost": {
      "type": "number"
    },
    "sku": {
      "type": "string",
      "maxLength": 50
    },
    "minutesToMake": {
      "type": "integer"
    },
    "type": {
      "type": "string",
      "maxLength": "50"
    },
    "updatedAt": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    }
  },
  "examples": [
    {
      "userId": 1,
      "name": "Sample Product",
      "description": "This is a sample product description.",
      "price": 19.99,
      "cost": 9.99,
      "sku": "SKU12345",
      "minutesToMake": 120,
      "type": "sticker",
      "updatedAt": "2024-05-25T12:34:56Z",
      "createdAt": "2024-05-25T12:34:56Z"
